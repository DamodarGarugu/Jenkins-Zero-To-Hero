pipeline {
  agent {
    docker { image 'openjdk:11' }
  }
  stages {
    stage('Test_Java_Ver') {
      steps {
        sh 'java --version'
      }
              stage('Run the Code'){
           steps{
                sh 'javac ./Test.java'
                sh 'java Test'
                }
            }
        }
    }
  }
